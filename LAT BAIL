BAIL INFO
SELECT DISTINCT
    OPENCLOCKSVIEW.DOCKET,
    OPENCLOCKSVIEW.DOCKETNO,
    OPENCLOCKSVIEW.HOLDINGCASE,
    JAILHOUSEVIEW.OFFENDER,
    DOCKETS.ARREST,
    DOCKETS.BAILSET,
    DOCKETS.BAIL
FROM
    OPENCLOCKSVIEW
JOIN
    DOCKETS ON OPENCLOCKSVIEW.OFFENDER = DOCKETS.OFFENDER
JOIN
    JAILHOUSEVIEW ON OPENCLOCKSVIEW.OFFENDER = JAILHOUSEVIEW.OFFENDER
WHERE
    DOCKETS.BAIL > 0
LIMIT 5000
