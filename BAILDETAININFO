SELECT
OPENCLOCKSVIEW.DOCKETNO,
JAILHOUSEVIEW.PPN,
JAILHOUSEVIEW.RNAME,
JAILHOUSEVIEW.LNAME,
JAILHOUSEVIEW.ONAME,
DOCKETS.DETAINER,
DOCKETS.BAILSET,
DOCKETS.BAIL
FROM OPENCLOCKSVIEW
JOIN DOCKETS ON OPENCLOCKSVIEW.DOCKETNO = DOCKETS.DOCKETNO
JOIN JAILHOUSEVIEW ON OPENCLOCKSVIEW.OFFENDER = JAILHOUSEVIEW.OFFENDER
